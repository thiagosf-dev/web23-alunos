{
  "name": "protochain",
  "version": "1.0.0",
  "description": "Protochain project.",
  "main": "index.js",
  "scripts": {
    "compile": "npx tsc",
    "dev": "npx nodemon --watch ./src/ --exec npx ts-node ./src/lib/blockchain.ts -e ts",
